<h2>Sign in</h2>
<%= form_with model: @user, url: users_path, local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
   <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit "Create Account" %>
  </div>
<% end %>

<%= link_to "Back to Homepage", root_path %>